<h1>Discover musics</h1>
<br />
<button class="btn btn-default"><span class="glyphicon glyphicon-plus-sign" aria-hidden="true"></span><%= link_to 'New Music', new_music_path %></button>

<div >
<br />  
<table class="table table-striped">
<% @musics.each do |music| %>
  <tr class="<%= cycle('list_line_odd', 'list_line_even') %>">
    <td>
      <%= image_tag("http://www.umflint.edu/sites/default/files/groups/CSEP/images/unknown_user_0.jpg", class: 'list_image') %>
    </td>
    <td class="list_description">
      <dl>
        <dt><%= music.comment %> <%= music.hashtag %></dt>        
        <dt id="title">&nbsp&nbsp&nbsp<%= music.title %> - <%= music.artist %></dt>
        <dd><iframe frameborder="no" border="0" marginwidth="0" marginheight="0" width=320 height=80 src="<%= music.link %>"></iframe></dd>
      </dl>
    </td>
    <td class="list_actions">
      <%= link_to 'Show', music %><br/>
      <%= link_to 'Edit', edit_music_path(music) %><br/>
      <%= link_to 'Delete', music, method: :delete,
                  data: { confirm: 'Are you sure?' } %>
    </td>
  </tr>
<% end %>
</table>
<br />

</div>